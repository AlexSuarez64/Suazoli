    "@types/jquery": "^3.3.29",
    "@types/karma": "^3.0.1",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.18",
    "codelyzer": "^4.5.0",
    "hads": "^1.7.1",
    "htmlhint": "^0.10.1",
    "https-proxy-agent": "^2.2.1",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-junit-reporter": "^1.2.0",
    "ngrx-store-freeze": "^0.2.4",
    "protractor": "~5.4.2",
    "puppeteer": "^1.11.0",
    "stylelint": "^9.10.1",

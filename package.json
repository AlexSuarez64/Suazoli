{
    "name": "ng-x-rocket",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "ng": "ng",
        "build": "npm run env -s && ng build --prod",
        "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
        "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
        "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
        "test": "npm run env -s && ng test",
        "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
        "e2e": "npm run env -s && ng e2e",
        "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
        "docs": "hads ./docs -o",
        "env": "ngx-scripts env npm_package_version",
        "generate": "ng generate"
    },
    "dependencies": {
        "@angular/animations": "^8.0.2",
        "@angular/cdk": "^8.0.1",
        "@angular/common": "^8.0.2",
        "@angular/compiler": "^8.0.2",
        "@angular/core": "^8.0.2",
        "@angular/flex-layout": "8.0.0-beta.26",
        "@angular/forms": "^8.0.2",
        "@angular/material": "^8.0.1",
        "@angular/material-moment-adapter": "^8.0.1",
        "@angular/platform-browser": "^8.0.2",
        "@angular/platform-browser-dynamic": "^8.0.2",
        "@angular/router": "^8.0.2",
        "@angular/service-worker": "^8.0.2",
        "@fortawesome/angular-fontawesome": "^0.4.0",
        "@fortawesome/fontawesome-free": "^5.9.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.19",
        "@fortawesome/free-solid-svg-icons": "^5.9.0",
        "@ngrx/data": "^8.0.1",
        "@ngrx/effects": "^8.0.1",
        "@ngrx/entity": "^8.0.1",
        "@ngrx/router-store": "^8.0.1",
        "@ngrx/store": "^8.0.1",
        "@ngrx/store-devtools": "^8.0.1",
        "@ngx-translate/core": "^11.0.1",
        "body-parser": "^1.19.0",
        "bootstrap": "^4.3.1",
        "chart.js": "2.8.0",
        "core-js": "^2.6.2",
        "dexie": "^2.0.4",
        "font-awesome": "^4.7.0",
        "fullcalendar": "^3.10.0",
        "hammerjs": "^2.0.8",
        "lodash": "^4.17.11",
        "material-design-icons-iconfont": "^4.0.4",
        "moment": "^2.23.0",
        "primeicons": "^1.0.0",
        "primeng": "^7.0.4",
        "quill": "^1.3.6",
        "reflect-metadata": "^0.1.12",
        "roboto-fontface": "^0.10.0",
        "rxjs": "^6.2.2",
        "rxjs-compat": "^6.3.3",
        "ts-helpers": "^1.1.2",
        "tslib": "^1.9.0",
        "zone.js": "^0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.800.3",
        "@angular-devkit/core": "^8.0.3",
        "@angular-devkit/schematics": "^8.0.3",
        "@angular/cli": "^8.0.3",
        "@angular/compiler-cli": "^8.0.2",
        "@angular/language-service": "^8.0.2",
        "@biesbjerg/ngx-translate-extract": "^2.3.4",
        "@ngrx/schematics": "^8.0.1",
        "@ngx-rocket/scripts": "^3.0.1",
        "@types/core-js": "^2.5.0",
        "@types/dexie": "1.3.1",
        "@types/jasmine": "^3.3.5",
        "@types/jasminewd2": "^2.0.6",
        "@types/jquery": "^3.3.29",
        "@types/karma": "^3.0.1",
        "@types/lodash": "^4.14.119",
        "@types/node": "^10.12.18",
        "codelyzer": "^4.5.0",
        "hads": "^1.7.1",
        "htmlhint": "^0.10.1",
        "https-proxy-agent": "^2.2.1",
        "jasmine-core": "^3.3.0",
        "jasmine-spec-reporter": "^4.2.1",
        "karma": "^3.1.4",
        "karma-chrome-launcher": "^2.2.0",
        "karma-cli": "^2.0.0",
        "karma-coverage-istanbul-reporter": "^2.0.4",
        "karma-jasmine": "^2.0.1",
        "karma-junit-reporter": "^1.2.0",
        "protractor": "~5.4.2",
        "puppeteer": "^1.11.0",
        "stylelint": "^9.10.1",
        "stylelint-config-recommended-scss": "^3.2.0",
        "stylelint-config-standard": "^18.2.0",
        "stylelint-scss": "^3.5.0",
        "ts-node": "^7.0.1",
        "tslint": "^5.12.1",
        "typescript": "3.4.5"
    }
}

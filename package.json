{
  "name": "ng-x-rocket",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^7.2.0",
    "@angular/common": "^7.2.0",
    "@angular/compiler": "^7.2.0",
    "@angular/core": "^7.2.0",
    "@angular/forms": "^7.2.0",
    "@angular/http": "^7.2.0",
    "@angular/platform-browser": "^7.2.0",
    "@angular/platform-browser-dynamic": "^7.2.0",
    "@angular/router": "^7.2.0",
    "@ngx-translate/core": "^11.0.1",
    "@angular/service-worker": "^7.2.0",
    "@angular/cdk": "^7.2.1",
    "@angular/material": "^7.2.1",
    "@angular/material-moment-adapter": "^7.2.1",
    "@angular/flex-layout": "7.0.0-beta.19",
    "material-design-icons-iconfont": "^4.0.4",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@ngrx/effects": "^7.0.0",
    "@ngrx/entity": "^7.0.0",
    "@ngrx/router-store": "^7.0.0",
    "@ngrx/store": "^7.0.0",
    "@ngrx/store-devtools": "^7.0.0",
    "angular-in-memory-web-api": "^0.8.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.2.1",
    "chart.js": "2.7.3",
    "core-js": "^2.6.2",
    "dexie": "^2.0.4",
    "font-awesome": "^4.7.0",
    "fullcalendar": "^3.10.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "primeicons": "^1.0.0",
    "primeng": "^7.0.4",
    "quill": "^1.3.6",
    "reflect-metadata": "^0.1.12",
    "roboto-fontface": "^0.10.0",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.3.3",
    "ts-helpers": "^1.1.2",
    "zone.js": "^0.8.27"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.1",
    "@angular-devkit/core": "^7.2.1",
    "@angular-devkit/schematics": "^7.2.1",
    "@angular/cli": "^7.2.1",
    "@angular/compiler": "^7.2.0",
    "@angular/compiler-cli": "^7.2.6",
    "@angular/language-service": "^7.2.0",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngrx/schematics": "^7.0.0",
    "@ngx-rocket/scripts": "^3.0.1",
